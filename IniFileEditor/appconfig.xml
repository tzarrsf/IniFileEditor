	<entry key="ini1FileNamePattern">setup.ini</entry>
	<entry key="ini1SectionName">connect1</entry>
	<entry key="ini1RestrictSearchToDirectories">C:\Mckesson\client</entry>
	<entry key="ini1host">mckprod.cloud.retail.net</entry>
	<!-- Second INI File change set --> 
	<entry key="ini2FileNamePattern">tomax.ini</entry>
	<entry key="ini2SectionName">txrep</entry>
	<entry key="ini2RestrictSearchToDirectories">C:\Mckesson\data</entry>
	<entry key="ini2host">mckprod.cloud.retail.net</entry>
</properties>